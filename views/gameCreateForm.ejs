

<% include partials/header %>

<form action="/game/insert">
    <label for="title">Title</label> <input type="text" id="title" name="title" /><br />
    <label for="description">Description</label> <input type="text" id="description" name="description" /><br />

    ESRB Rating
    <select name="rating_id">
        <% for( var i = 0; i < esrb.length; i++) { %>
        <option
                name="rating_id" id="rating_id" value="<%= esrb[i].rating_id %>"><%= esrb[i].rating %>
            <% } %>
    </select>

<br /><br />
    <button id="createGame">Create Game</button>
</form>

<% include partials/footer %>