<% include ./partials/header.ejs %>
<div>
    <% if(rs.length > 0) { %>
    <table border="1">
        <tr>
            <th>Name</th>
        </tr>
        <% for (var i=0; rs.length > i; i++) { %>
        <tr>
            <td><a href="/users/?user_id=<%= rs[i].user_id %>"> <%= rs[i].username %></a></td>
        </tr>
        <% } %>
    </table>
    <% } else { %>
    No users exist.
    <% } %>
</div>

<% include ./partials/footer.ejs %>